{
  "kind": "collectionType",
  "collectionName": "kategorien",
  "info": {
    "singularName": "kategorie",
    "pluralName": "kategorien",
    "displayName": "News Kategorien",
    "description": "Kategorien f√ºr News-Artikel"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "beschreibung": {
      "type": "text"
    },
    "farbe": {
      "type": "string",
      "regex": "^#([A-Fa-f0-9]{6}|[A-Fa-f0-9]{3})$"
    },
    "reihenfolge": {
      "type": "integer",
      "default": 0
    },
    "news_artikel": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::news-artikel.news-artikel",
      "mappedBy": "kategorie"
    }
  }
}
