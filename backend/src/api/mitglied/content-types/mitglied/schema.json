{
  "kind": "collectionType",
  "collectionName": "mitglieder",
  "info": {
    "singularName": "mitglied",
    "pluralName": "mitglieder",
    "displayName": "Mitglied",
    "description": "Vereinsmitglieder mit Website-Account-Integration"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "vorname": {
      "type": "string",
      "required": true,
      "maxLength": 50
    },
    "nachname": {
      "type": "string",
      "required": true,
      "maxLength": 50
    },
    "mitgliedsnummer": {
      "type": "string",
      "required": true,
      "unique": true,
      "maxLength": 20
    },
    "email": {
      "type": "email"
    },
    "telefon": {
      "type": "string",
      "maxLength": 20
    },
    "adresse": {
      "type": "text"
    },
    "geburtsdatum": {
      "type": "date"
    },
    "eintrittsdatum": {
      "type": "date"
    },
    "mitgliedsart": {
      "type": "enumeration",
      "enum": ["Aktiv", "Passiv", "Jugend", "Ehrenmitglied"],
      "required": true,
      "default": "Aktiv"
    },
    "status": {
      "type": "enumeration",
      "enum": ["Aktiv", "Inaktiv", "Gek√ºndigt"],
      "required": true,
      "default": "Aktiv"
    },
    "website_user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "notizen": {
      "type": "text",
      "private": true
    },
    "oeffentlich_sichtbar": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "spieler": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::spieler.spieler",
      "mappedBy": "mitglied"
    }
  }
}