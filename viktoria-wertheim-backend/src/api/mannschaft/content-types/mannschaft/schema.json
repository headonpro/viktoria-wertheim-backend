{
  "kind": "collectionType",
  "collectionName": "mannschaften",
  "info": {
    "singularName": "mannschaft",
    "pluralName": "mannschaften",
    "displayName": "Mannschaft",
    "description": "Vereinsmannschaften und Teams"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "liga": {
      "type": "enumeration",
      "enum": [
        "Kreisklasse B",
        "Kreisklasse A",
        "Kreisliga",
        "Landesliga"
      ],
      "required": true
    },
    "trainer": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::mitglied.mitglied",
      "inversedBy": "trainer_mannschaften"
    },
    "teamfoto": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images"
      ]
    },
    "beschreibung": {
      "type": "richtext"
    },
    "gruendungsjahr": {
      "type": "integer",
      "min": 1900,
      "max": 2030
    },
    "vereinsfarben": {
      "type": "string"
    },
    "spielort": {
      "type": "string",
      "default": "Sportplatz Wertheim"
    },
    "saison": {
      "type": "string",
      "default": "2024/25"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "aktiv",
        "inaktiv",
        "aufgeloest"
      ],
      "required": true
    },
    "altersklasse": {
      "type": "enumeration",
      "enum": [
        "senioren",
        "a-jugend",
        "b-jugend",
        "c-jugend",
        "d-jugend",
        "e-jugend",
        "f-jugend",
        "bambini"
      ],
      "required": true
    },
    "kontaktperson": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::mitglied.mitglied",
      "inversedBy": "kontakt_mannschaften"
    },
    "trainingszeiten": {
      "type": "text"
    },
    "bemerkungen": {
      "type": "text"
    },
    "tabellenplatz": {
      "type": "integer",
      "default": 1,
      "min": 1,
      "max": 20
    },
    "punkte": {
      "type": "integer",
      "default": 0,
      "min": 0
    },
    "spiele_gesamt": {
      "type": "integer",
      "default": 0,
      "min": 0
    },
    "siege": {
      "type": "integer",
      "default": 0,
      "min": 0
    },
    "unentschieden": {
      "type": "integer",
      "default": 0,
      "min": 0
    },
    "niederlagen": {
      "type": "integer",
      "default": 0,
      "min": 0
    },
    "tore_fuer": {
      "type": "integer",
      "default": 0,
      "min": 0
    },
    "tore_gegen": {
      "type": "integer",
      "default": 0,
      "min": 0
    },
    "tordifferenz": {
      "type": "integer",
      "default": 0
    },
    "form_letzte_5": {
      "type": "json"
    },
    "trend": {
      "type": "enumeration",
      "default": "gleich",
      "enum": [
        "steigend",
        "gleich",
        "fallend"
      ],
      "required": true
    },
    "liga_vollname": {
      "type": "string"
    },
    "trainers": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::trainer.trainer",
      "mappedBy": "mannschafts"
    }
  }
}