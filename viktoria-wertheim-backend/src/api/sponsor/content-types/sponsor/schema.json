{
  "kind": "collectionType",
  "collectionName": "sponsors",
  "info": {
    "singularName": "sponsor",
    "pluralName": "sponsors",
    "displayName": "Sponsor"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "maxLength": 100,
      "required": true
    },
    "logo": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "website_url": {
      "type": "string",
      "maxLength": 255,
      "required": false
    },
    "beschreibung": {
      "type": "text",
      "maxLength": 500
    },
    "kategorie": {
      "type": "enumeration",
      "enum": [
        "hauptsponsor",
        "premium",
        "partner",
        "default:partner"
      ]
    },
    "reihenfolge": {
      "type": "integer",
      "default": 1,
      "min": 1,
      "max": 999
    },
    "aktiv": {
      "type": "boolean",
      "default": true
    }
  }
}
